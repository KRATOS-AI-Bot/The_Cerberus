
{
    "signatures": [
        {
            "name": "AWS Access Key ID",
            "pattern": "AKIA[0-9A-Z]{16}"
        },
        {
            "name": "AWS Secret Access Key",
            "pattern": "aws_secret_access_key = [a-zA-Z0-9/+=]{40}"
        },
        {
            "name": "Gemini API Key",
            "pattern": "gemini_api_key = [a-zA-Z0-9]{32}"
        },
        {
            "name": "OpenAPI Key",
            "pattern": "openapi_key = [a-zA-Z0-9]{32}"
        },
        {
            "name": "Groq API Key",
            "pattern": "groq_api_key = [a-zA-Z0-9]{32}"
        },
        {
            "name": "GitHub Token",
            "pattern": "ghp_[a-zA-Z0-9]{36}"
        },
        {
            "name": "Slack Token",
            "pattern": "xoxb-[a-zA-Z0-9]{32}-[a-zA-Z0-9]{32}"
        },
        {
            "name": "Discord Token",
            "pattern": "discord_token = [a-zA-Z0-9]{32}"
        },
        {
            "name": "RSA Private Key",
            "pattern": "-----BEGIN RSA PRIVATE KEY-----[a-zA-Z0-9/+=\\n]{500}-----END RSA PRIVATE KEY-----"
        },
        {
            "name": "DSA Private Key",
            "pattern": "-----BEGIN DSA PRIVATE KEY-----[a-zA-Z0-9/+=\\n]{500}-----END DSA PRIVATE KEY-----"
        },
        {
            "name": "Stripe API Key",
            "pattern": "sk_live_[a-zA-Z0-9]{32}"
        },
        {
            "name": "RazorPay API Key",
            "pattern": "razorpay_api_key = [a-zA-Z0-9]{32}"
        }
    ]
}
