
{
    "signatures": [
        {
            "name": "AWS Access Key ID",
            "pattern": "AKIA[0-9A-Z]{16}"
        },
        {
            "name": "AWS Secret Access Key",
            "pattern": "([0-9a-zA-Z/+=]{40})"
        },
        {
            "name": "Gemini API Key",
            "pattern": "gemini_api_key=[a-zA-Z0-9_\\-]+"
        },
        {
            "name": "OpenAPI Key",
            "pattern": "api_key=[a-zA-Z0-9_\\-]+"
        },
        {
            "name": "Groq API Key",
            "pattern": "groq_api_key=[a-zA-Z0-9_\\-]+"
        },
        {
            "name": "GitHub Token",
            "pattern": "ghp_[a-zA-Z0-9_\\-]+"
        },
        {
            "name": "Slack Token",
            "pattern": "xoxb-[a-zA-Z0-9_\\-]+"
        },
        {
            "name": "Discord Token",
            "pattern": "discord_token=[a-zA-Z0-9_\\-]+"
        },
        {
            "name": "RSA Private Key",
            "pattern": "-----BEGIN RSA PRIVATE KEY-----[a-zA-Z0-9/\\+\\n]+-----END RSA PRIVATE KEY-----"
        },
        {
            "name": "DSA Private Key",
            "pattern": "-----BEGIN DSA PRIVATE KEY-----[a-zA-Z0-9/\\+\\n]+-----END DSA PRIVATE KEY-----"
        },
        {
            "name": "Stripe Key",
            "pattern": "sk_live_[a-zA-Z0-9_\\-]+"
        },
        {
            "name": "RazorPay Key",
            "pattern": "razorpay_api_key=[a-zA-Z0-9_\\-]+"
        }
    ]
}
